.toast{
  position: fixed;
  top: 0;
  z-index: 100;
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  opacity: 0;
}
.toast-in{
  background: rgba(1, 1, 1, .7);
  min-width: 30%;
  max-width: 38%;
  padding: 10rpx 10rpx 40rpx;
  box-sizing: border-box;
  border-radius: 16rpx;
}
.toast_icon{
  width: 124rpx;
  height: 124rpx;
  margin: 20rpx auto 10rpx;
}
.toast_icon > image{
  width: 100%;
  height: 100%;
}
.toast_content{
  text-align: center;
  font-size: 28rpx;
  line-height: 40rpx;
  color: #F56B25;
}