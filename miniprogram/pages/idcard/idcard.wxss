.idcard-model{
  margin-top: 110rpx;
}
.idcard-container .wrap{
  padding: 0 60rpx;
}
.idcard-model image{
  width: 100%;
}
.idcard-content{
  text-align: center;
  margin-top: 80rpx;
}
.idcard-content-h{
  font-size: 48rpx;
  color: #FFFFFF;
}
.idcard-content-p{
  font-size: 32rpx;
  line-height: 50rpx;
  color: rgba(255,255,255,0.65);
  margin-top: 30rpx;
}
.idcard-btn{
  position: fixed;
  left: 0;
  bottom: 150rpx;
  width: 100%;
  text-align: center;
}
.idcard-btn .health-btn{
  margin: 0 auto;
}
.camera-con{
  margin-top: 110rpx;
}
.idcard-camera,.camera-img{
  width: 100%;
  height: 240px;
  border-radius: 16rpx;
}
.idcard-go{
  display: inline-block;
}
.idcard-agreement{
  position: fixed;
  left: 0;
  bottom: 80rpx;
  width: 100%;
  text-align: center;
  font-size: 24rpx;
  color: #fff;
  opacity: 0.65;
}
